<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dao.ContactDao">

    <insert id="addContact" parameterType="Pojo.Contact">
        INSERT INTO ct_contact (uconttactid,ucustomerid,ucontactperson,ucontactphone,ucontactfax,ucontactmail)VALUES
        (#{uconttactid},#{ucustomerid},#{ucontactperson},#{ucontactphone},#{ucontactfax},#{ucontactmail})
    </insert>

</mapper>